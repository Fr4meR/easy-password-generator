    <string name="help_content_templates_internet_passwords">
        <![CDATA[
            <p>Use this template to create memorable passwords.</p>
            <p>
                It is possible to set the password length for this template. The generated passwords
                will consist of a capital letter followed by lowercase letters and three digits
                at the end. Furthermore the password is talking - this means, that the password
                consists alternately of a vowel and a consonant.
            </p>
        ]]>
    </string>
    <string name="help_content_templates_complex_passwords">
        <![CDATA[
            <p>Use this template to create complex passwords.</p>
            <p>
                It is possible to set the password length for this template. The generated
                passwords will consist of upper- and lowercase letters, digits and special characters.
            </p>
        ]]>
    </string>
    <string name="help_content_templates_wep_keys">
        <![CDATA[
            <p>Use this template to create WEP-Keys.</p>
            <p>
                It is not possible to set the password length. The generated passwords can be used
                for WEP encryption.
            </p>
        ]]>
    </string>
    <string name="help_content_templates_wpa2_keys">
        <![CDATA[
            <p>Use this template to create WPA2-Keys.</p>
            <p>
                It is not possible to set the password length. The generated passwords can be used
                for WPA2 encryption.
            </p>
        ]]>
    </string>

